{
  "expo": {
    "name": "ToDoApp",
    "slug": "ToDoApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Todo.png",
    "scheme": "todoapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#72b4ed"
        // "foregroundImage": "./assets/images/android-icon-foreground.png",
        // "backgroundImage": "./assets/images/android-icon-background.png",
        // "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.arunfernando.ToDoApp"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/Bot.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/Lap.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "73f6b0b3-3a12-4df9-b297-954ad33404a5"
      }
    }
  }
}
